<?xml version="1.0"?>

<report	xmlns="http://www.w3schools.com"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <htmlTitle>My Best Report Ever</htmlTitle>

    <bodyCss>
  	    <cssRule>
  	        <ruleName>width</ruleName>
  	        <ruleValue>90%</ruleValue>
  	    </cssRule>
  	    <cssRule>
  	        <ruleName>margin</ruleName>
  	        <ruleValue>0 auto</ruleValue>
  	    </cssRule>
  	    <cssRule>
  	        <ruleName>padding</ruleName>
  	        <ruleValue>20px</ruleValue>
  	    </cssRule>
	</bodyCss>
	
  	<cssScript>
  	    #text1 h1{ margin: 0 auto; }
  	</cssScript>
    
    <textContent id="text1">
	  	<css>
	  	    <cssRule>
	  	        <ruleName>padding</ruleName>
	  	        <ruleValue>10px 0px</ruleValue>
	  	    </cssRule>
		</css>
		
        <content>
            <title>Carpe Diem</title>
            <paragraph>Dum loquimur fugerit invida
				aetas: carpe diem, quam minimum credula postero.</paragraph>
        </content>
		<content>
            <title>Acta est fabula</title>
            <paragraph>The end of the show</paragraph>
        </content>
    </textContent>
    
    
    <lineChart id="chart1">
        
        <title>Line Chart Title</title>
        
   	  	<css>
	  	    <cssRule>
	  	        <ruleName>padding</ruleName>
	  	        <ruleValue>10px 0px</ruleValue>
	  	    </cssRule>
		</css>

   	  	<showLegend>true</showLegend>
        
        <line>
            <lineWidth>5</lineWidth>
            
			<point>
				<X>0</X>
				<Y>1</Y>
            </point>
			
			<point>
				<X>1</X>
				<Y>2</Y>                    
            </point>
            <point>
				<X>2</X>
				<Y>3</Y>                    
            </point>
            <point>
				<X>4</X>
				<Y>5</Y>                    
			</point>
               
        </line>
    </lineChart>

    <table id="table1">
        <title>My new Table</title>

	  	<css>
	  	    <cssRule>
	  	        <ruleName>padding</ruleName>
	  	        <ruleValue>20px 0px</ruleValue>
	  	    </cssRule>
		</css>
		
		<htmlClass>lalaHtmlClass</htmlClass>
	    
        <col>
            <header>Header 1</header>
            <value>1</value>
            <value>2</value>
			<value>3</value>
			<value>4</value>
            <value>5</value>
		</col>
		
        <col>
            <header>Header 2</header>
            <value>1</value>
            <value>2</value>
			<value>3</value>
			<value>4</value>
            <value>5</value>
		</col>
		
    </table>
    
    <barChart id="chart3">
        <title>Bars</title>
	  	<css>
	  	    <cssRule>
	  	        <ruleName>padding</ruleName>
	  	        <ruleValue>10px 0px</ruleValue>
	  	    </cssRule>
		</css>
		
        <showLegend>true</showLegend>

        <tick>May</tick>
        <tick>June</tick>
		<tick>July</tick>
        
        <barSeries>
            <title>Hotel</title>
            <value>200</value>
            <value>600</value>
            <value>550</value>
        </barSeries>
        
        <barSeries>
            <title>B&amp;B</title>
            <value>800</value>
            <value>1000</value>
            <value>150</value>
        </barSeries>

        <barSeries>
            <title>Hostel</title>
            <value>-200</value>
            <value>100</value>
            <value>5</value>
        </barSeries>
        
    </barChart>	
    
    <pieChart id="chart4">
        <title>Piee</title>
	  	<css>
	  	    <cssRule>
	  	        <ruleName>padding</ruleName>
	  	        <ruleValue>10px 0px</ruleValue>
	  	    </cssRule>
		</css>
		
        <showLegend>true</showLegend>        
        
        <pieSeries>
            <title>Cupcakes</title>
            <value>40</value>
        </pieSeries>
        
        <pieSeries>
            <title>Apples</title>
            <value>7</value>
        </pieSeries>

        <pieSeries>
            <title>Peaches</title>
            <value>3</value>
        </pieSeries>
        
        <pieSeries>
            <title>Muffins</title>
            <value>40</value>
        </pieSeries>

        <pieSeries>
            <title>Bananas</title>
            <value>10</value>
        </pieSeries>
        
	</pieChart>
	
    <lineChart id="chart5">
        
        <title>Line Chart Title</title>
        <showLegend>true</showLegend>

        <line>
            <lineLabel>First Line</lineLabel>
            <lineColor>orange</lineColor>
            <lineWidth>5</lineWidth>
            <showLine>true</showLine>

            <markerOptions>
                <style>diamond</style>
                <color>black</color>
            </markerOptions>
            
            <point>
                <X>1</X>
                <Y>2</Y>                    
           </point>
           <point>
                <X>2</X>
                <Y>3</Y>                    
           </point>
           <point>
                <X>4</X>
                <Y>5</Y>                    
           </point>    
        </line>

        <line>
            <lineLabel>Second Line</lineLabel>
            <lineWidth>6</lineWidth>
            <showLine>true</showLine>
            <lineColor>blue</lineColor>

            <markerOptions>
                <style>square</style>
                <color>black</color>
			</markerOptions>
            
            <point>
                <X>2</X>
                <Y>5</Y>                    
           </point>
           <point>
                <X>3</X>
                <Y>8</Y>                    
           </point>
           <point>
                <X>1</X>
                <Y>4</Y>                    
           </point>    
        </line>
    </lineChart>
</report>